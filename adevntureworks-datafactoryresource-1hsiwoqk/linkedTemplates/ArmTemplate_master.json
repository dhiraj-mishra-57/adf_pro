					},
					"adls_gen2_adfpro_accountKey": {
						"value": "[parameters('adls_gen2_adfpro_accountKey')]"
					},
					"AzureDatabricks_properties_typeProperties_existingClusterId": {
						"value": "[parameters('AzureDatabricks_properties_typeProperties_existingClusterId')]"
					},
					"RestService_LC_properties_typeProperties_url": {
						"value": "[parameters('RestService_LC_properties_typeProperties_url')]"
					},
					"adfprosqldatabase_properties_typeProperties_server": {
						"value": "[parameters('adfprosqldatabase_properties_typeProperties_server')]"
					},
					"adfprosqldatabase_properties_typeProperties_database": {
						"value": "[parameters('adfprosqldatabase_properties_typeProperties_database')]"
					},
					"adfprosqldatabase_properties_typeProperties_userName": {
						"value": "[parameters('adfprosqldatabase_properties_typeProperties_userName')]"
					},
					"adls_gen2_adfpro_properties_typeProperties_url": {
						"value": "[parameters('adls_gen2_adfpro_properties_typeProperties_url')]"
					}
				}
